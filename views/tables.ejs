      <div class="activity-card">
        <h3>Table Occupancy</h3>

        <div class="table-responsive">
          <table>
            <thead>
              <tr>
                <th>Table Number</th>
                <th>Customer Name</th>
                <th>capacity</th>
                <th>Availability</th>


              </tr>
            </thead>


            <% for( let i=0; i < tables.length; i++ ) { %>
              <tr>
                <td>
                  <%= tables[i].tableID %>
                </td>
                <td>
                  <%=tables[i].BookedBy%>
                </td>
                <td>
                  <%=tables[i].Capacity%>
                </td>
                <td>
                  <%=tables[i].Availability%> <a href="myfunc()"><img
                        src="https://img.icons8.com/small/16/000000/change.png" id="change_status"
                        onclick="myfunc()" /></a>
                </td>
                <!-- <td><%=tables[i].Status%></td> -->
              </tr>
              <% } %>





